@{
}

<h1>Coding Events</h1>

<p>
    <a asp-controller="Events" asp-action="Add">Add Event</a>
</p>

<table class="table table-danger table-striped border border-dark boarder-3">
    <tr class-"bg-light"> 
        <th>Event Name</th>
        <th>Event Description</th>
    </tr>
    @foreach (KeyValuePair<string, string> meeting in ViewBag.events)
    {
        <tr>
        <td>@meeting.Key</td>
        <td>@meeting.Value</td>
        </tr>
    }
</table>